<?xml version="1.0"?>
<ruleset name="wp-browser code style" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:nonamespaceschemalocation="../../../phpcs.xsd">
    <description>
        The PSR-2 coding standard without the rule not allowing method names starting in underscore.
    </description>
    <arg name="tab-width" value="4">
    </arg>
    <rule ref="PSR2">
    </rule>
    <rule ref="PSR2">
        <exclude name="PSR2.Methods.MethodDeclaration.Underscore">
        </exclude>
    </rule>
</ruleset>
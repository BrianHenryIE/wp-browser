{
  "_hash": "11223344",
  "name": "lucatume/wp-browser",
  "type": "library",
  "description": "WordPress extension of the PhpBrowser class.",
  "keywords": [
    "wordpress",
    "codeception"
  ],
  "homepage": "https://github.com/lucatume/wp-browser",
  "license": "MIT",
  "authors": [
    {
      "name": "theAverageDev (Luca Tumedei)",
      "email": "luca@theaveragedev.com",
      "homepage": "https://theaveragedev.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "composer-runtime-api": "^2.2",
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "^2.0 || ^3.0",
    "codeception/module-phpbrowser": "^2.0 || ^3.0",
    "codeception/module-webdriver": "^2.0 || ^3.0",
    "codeception/module-db": "^2.0 || ^3.0",
    "codeception/module-filesystem": "^2.0 || ^3.0",
    "codeception/module-cli": "^2.0 || ^3.0",
    "opis/closure": "^3.5",
    "symfony/process": ">=4.4.24 <7.0",
    "vlucas/phpdotenv": "^5.0"
  },
  "require-dev": {
    "ext-sockets": "*",
    "ext-pcntl": "*",
    "ext-mysqli": "*",
    "lucatume/codeception-snapshot-assertions": "v1.0.0.x-dev",
    "gumlet/php-image-resize": "^1.6",
    "szepeviktor/phpstan-wordpress": "^1.3",
    "phpstan/extension-installer": "^1.3",
    "rector/rector": "^0.16.0"
  },
  "autoload": {
    "psr-4": {
      "lucatume\\WPBrowser\\": [
        "src/",
        "src/Deprecated"
      ],
      "Hautelook\\Phpass\\": "includes/Hautelook/Phpass"
    },
    "files": [
      "src/version-4-aliases.php",
      "src/Deprecated/deprecated-functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "lucatume\\WPBrowser\\Tests\\": "tests/_support"
    },
    "files": [
      "tests/_support/functions.php"
    ]
  },
  "extra": {
    "_hash": "484f861f69198089cab0e642f27e5653"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "scripts": {
    "stan": [
      "phpstan analyse --memory-limit=4G --no-progress --no-interaction --ansi -c config/phpstan.neon.dist"
    ],
    "rector-dry-run": [
      "rector process --ansi --config config/rector.php --autoload-file vendor/autoload.php --dry-run"
    ],
    "rector": [
      "rector process --ansi --config config/rector.php --autoload-file vendor/autoload.php"
    ]
  }
}

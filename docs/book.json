{
  "title": "wp-browser docs",
  "description": "Setup and API documentation for the wp-browser project",
  "author": "Luca Tumedei",
  "structure": {
    "readme": "welcome.md",
    "summary": "summary.md"
  },
  "plugins": [
    "expandable-chapters",
    "simpletabs",
    "code",
    "prism",
    "-highlight",
    "atoc",
    "codeblock-filename",
    "versions-select"
  ],
  "pluginsConfig": {
    "atoc": {
      "addClass": true,
      "className": "atoc"
    },
    "versions":
    {
      "gitbookConfigURL": "https://raw.githubusercontent.com/lucatume/wp-browser/doc-v2.2/docs/book.json",
      "options": [
        {
          "value": "/",
          "text": "Version 2.2"
        }
      ]
    }
  }
}
